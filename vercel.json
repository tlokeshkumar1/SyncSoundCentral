{
  "version": 2,
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "outputDirectory": "dist/public",
  "buildCommand": "npm run build:vercel"
}
